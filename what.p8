__lua__
-- w h a t
-- by rileyjshaw
music(0)

x_pad=6
y_pad=8
fade={0,1,5,13,6,7}
fade_in={4,2,0}
turns=0

words={children={{
 name="what_",
 children={
		{
		 name="happened?",
		},
		{
		 name="about",
		 children={
		  {
		   name="breaking up?",
		  },
		  {
		   name="our",
		   children={
		    {
		     name="union?",
		    },
		    {
		     name="relationship?",
		    },
		   },
		  },
		  {
		   name="us?",
		  },
		  {
		   name="this",
		   children={
		    {
		     name="hurts?",
		    },
		    {
		     name="is",
		     children={
		      {
		       name="so",
		       children={
		        {
		         name="overwhelming?",
		        },
		       },
		      },
		     },
		    },
		    {
		     name="safety?",
		    },
		   },
		  },
		 },
		},
		{
		 name="took",
		 children={
		  {
		   name="over?",
		  },
		  {
		   name="our",
		   children={
		    {
		     name="unity?",
		    },
		    {
		     name="realm?",
		    },
		   },
		  },
		  {
		   name="knowing?",
		  },
		 },
		},
		{
		 name="now?"
		},
 },
}}}

word_1=words.children[1].name

function _init()
 turns+=1
 fader=-30
 hist={1}
 hist_words={}
 update_state()
end

function _update()
 if fader!=0 then
  if(ended()) return
  sign=mid(-1,fader,1)
  fader-=sign
  if(fader==0 and sign==1) then
   _init()
  end
  return
 end

 if btnp(4) or btnp(5) then
  push(hist_words,word)
  push(hist,char_idx-1)
  update_state()
  return
 end

 if(btnp(0)) char_idx-=1
 if(btnp(1)) char_idx+=1
 local top=#word
 if(hide_secret()) top-=1
 char_idx=mid(2,char_idx,top)
end

function _draw()
	cls()
	for c=1,#hist-1 do
	 print_word(hist_words[c],c)
	end
 print_word(word,#hist)
end

function update_state()
 y_0=offset(#hist,y_pad)
 char_idx=2
 local leaf=words
 foreach(hist,function(i)
  leaf=leaf.children[i]
 end)
 word=leaf.name
 if not leaf.children then
  char_idx=-1
  fader=180
  if ended() then
   music(3)
  else
   music(1)
  end
 end
end

function offset(n,size)
 return 64-(n+1.5)*size/2
end

function print_word(word,h)
 local is_what=h==1
 local is_active=false
 local y=y_0+h*y_pad

 local bump=is_what and 1 or 0
 local chars=#word-bump
 local x_0=offset(chars,x_pad)

 local word_col=7
 if fader>0 then
  local i_f=flr((fader-40)/4)+1
  i_f=mid(1,i_f,#fade)
  word_col=fade[i_f]
 elseif fader<0 then
  local i_f=flr(-fader/2)+1
  i_f=mid(1,i_f,#fade_in)
  word_col=fade_in[i_f]
 else is_active=h==#hist
 end
 if(is_active) word_col=14

 for c=1,#word do
  local disabled=is_active and
   c==1
  local selected=is_active and
   c==char_idx

  local char_col=
   disabled and 14 or
   selected and 8 or
   word_col

  if is_what and c==#word then
   if selected and not
     hide_secret() then
    char_col=8
   else
    char_col=0
   end
  end

  local char=sub(word,c,c)
  local x=x_0+c*x_pad
  print(char,x,y,char_col)
 end
end

function hide_secret()
 return #hist==1 and turns<3
end

function ended()
 return #hist==2 and (hist[2]==
  #word_1-1)
end

function push(arr,val)
 arr[#arr+1]=val
end

